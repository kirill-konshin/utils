{
  "name": "@kirill.konshin/electron-utils",
  "version": "0.0.5",
  "type": "module",
  "main": "build/index.js",
  "module": "build/index.js",
  "types": "build/index.d.ts",
  "exports": {
    ".": "./build/index.js",
    "./updater": "./build/updater.js",
