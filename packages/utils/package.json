{
  "name": "@kirill.konshin/utils",
  "description": "Utilities",
  "version": "0.0.19",
  "type": "module",
  "scripts": {
    "----- BUILD -----": "",
    "clean": "rm -rf dist .tscache tsconfig.tsbuildinfo",
    "build": "vite build",
    "build:index": "cti create ./src",
    "build:check-types": "attw --pack .",
    "start": "yarn build --watch",
    "wait": "wait-on ./dist/index.js",
    "----- TEST -----": "",
    "test": "vitest run --coverage",
    "test:watch": "vitest watch --coverage",
    "----- STORYBOOK -----": "",
    "storybook:start": "storybook dev -p 6006",
    "storybook:build": "storybook build"
  },
  "dependencies": {
    "@kirill.konshin/bootstrap": "*",
    "@kirill.konshin/core": "*",
    "@kirill.konshin/electron": "*",
    "@kirill.konshin/electron-builder": "*",
    "@kirill.konshin/mui": "*",
    "@kirill.konshin/next": "*",
    "@kirill.konshin/react": "*",
    "@kirill.konshin/react-native": "*",
    "@kirill.konshin/tailwind": "*"
  },
  "devDependencies": {
    "@kirill.konshin/utils-private": "*"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "Kirill Konshin <kirill@konshin.org> (https://konshin.org)",
  "license": "MIT",
  "exports": {
    "./tailwind": {
      "import": {
        "import": "./dist/tailwind/index.js",
        "types": "./dist/tailwind/index.d.ts"
      }
    },
    "./react-native": {
      "import": {
        "import": "./dist/react-native/index.js",
        "types": "./dist/react-native/index.d.ts"
      }
    },
    "./react": {
      "import": {
        "import": "./dist/react/index.js",
        "types": "./dist/react/index.d.ts"
      }
    },
    "./next": {
      "import": {
        "import": "./dist/next/index.js",
        "types": "./dist/next/index.d.ts"
      }
    },
    "./mui": {
      "import": {
        "import": "./dist/mui/index.js",
        "types": "./dist/mui/index.d.ts"
      }
    },
    "./electron-builder": {
      "import": {
        "import": "./dist/electron-builder/index.js",
        "types": "./dist/electron-builder/index.d.ts"
      }
    },
    "./electron": {
      "import": {
        "import": "./dist/electron/index.js",
        "types": "./dist/electron/index.d.ts"
      }
    },
    "./core": {
      "import": {
        "import": "./dist/core/index.js",
        "types": "./dist/core/index.d.ts"
      }
    },
    "./bootstrap": {
      "import": {
        "import": "./dist/bootstrap/index.js",
        "types": "./dist/bootstrap/index.d.ts"
      }
    },
    ".": {
      "import": {
        "import": "./dist/index.js",
        "types": "./dist/index.d.ts"
      }
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "peerDependenciesMeta": {}
}
