{
  "$schema": "https://swc.rs/schema.json",
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": true
    },
    "target": "es2020",
    "keepClassNames": true
  },
  "module": {
    "type": "es6",
    "strict": true,
    "preserveImportMeta": true
  },
  "sourceMaps": true,
  "exclude": [".*\\.stories\\.ts$",".*\\.stories\\.tsx$",".*\\.test\\.ts$"]
}